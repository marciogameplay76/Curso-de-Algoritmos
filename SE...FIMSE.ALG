Algoritmo "se...fimse"
// se... fimse
// se...senao
// escolha...caso
Var
  idade: inteiro


Inicio
   escreva("Por favor, digite sua idade: ")
   leia (idade)
   
   se idade >= 44 entao
      escreva ("Voce e maior de idade! ")
      fimse
      
   se idade < 44 entao
      escreva ("Voce e menor de idade! ")
      fimse


Fimalgoritmo